--THis code takes the imported csv, and makes it 
--the correct dimensions of the master table

SELECT count(*) FROM pragma_table_info('import3');



CASE 
WHEN SELECT count(*) FROM pragma_table_info('import3') < 25 
THEN ALTER TABLE import3 ADD COLUMN UserName9
ELSE ADD COLUMN bogus
END;


UPDATE import3
CASE
WHEN 5 == 5 Then Alter Table import3 ADD COLUMN UserName100
ELSE Select * import3
END;

